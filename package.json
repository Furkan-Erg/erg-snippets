{
  "name": "erg-snippets",
  "displayName": "Erg Snippets",
  "description": "This is snippets for React, Angular, Next, with both typescript and javascript supported snippets of common used components and pages with the help of tailwind styling",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.92.2"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/js.snippets.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/jsx.snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/tsx.snippets.code-snippets"
      }
    ]
  }
}
