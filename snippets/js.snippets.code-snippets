  "Tabs": {
    "prefix": "erg-tabs",
    "body": [
      "import React, { useState } from 'react';",
      "",
      "const Tabs = ({ tabs, defaultTab = 0 }) => {",
      "  const [activeTab, setActiveTab] = useState(defaultTab);",
      "",
      "  return (",
      "    <div>",
