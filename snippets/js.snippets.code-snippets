  "Button": {
    "prefix": "erg-button",
    "body": [
      "import React from 'react';",
      "",
      "const Button = ({ ",
      "  onClick, ",
      "  text, ",
      "  variant = 'primary', ",
      "  size = 'medium',",
      "  disabled = false,",
      "  fullWidth = false ",
      "}) => {",
      "  const variants = {",
      "    primary: { bg: '#4f46e5', hover: '#4338ca', text: 'white' },",
      "    secondary: { bg: '#6b7280', hover: '#4b5563', text: 'white' },",
      "    success: { bg: '#16a34a', hover: '#15803d', text: 'white' },",
      "    danger: { bg: '#dc2626', hover: '#b91c1c', text: 'white' },",
      "    outline: { bg: 'transparent', hover: '#eef2ff', text: '#4f46e5', border: '1px solid #4f46e5' },",
      "  };",
      "",
      "  const sizes = {",
      "    small: { padding: '0.25rem 0.75rem', fontSize: '0.875rem' },",
      "    medium: { padding: '0.5rem 1rem', fontSize: '1rem' },",
      "    large: { padding: '0.75rem 1.5rem', fontSize: '1.125rem' },",
      "  };",
      "",
      "  const v = variants[variant] || variants.primary;",
      "  const s = sizes[size] || sizes.medium;",
      "",
      "  return (",
      "    <button",
      "      style={{",
      "        ...s,",
      "        backgroundColor: disabled ? '#9ca3af' : v.bg,",
      "        color: v.text,",
      "        borderRadius: '0.375rem',",
      "        border: v.border || 'none',",
      "        cursor: disabled ? 'not-allowed' : 'pointer',",
      "        transition: 'all 0.2s',",
      "        width: fullWidth ? '100%' : 'auto',",
      "        fontWeight: 500,",
      "      }}",
      "      onClick={onClick}",
      "      disabled={disabled}",
      "      onMouseOver={(e) => !disabled && (e.target.style.backgroundColor = v.hover)}",
      "      onMouseOut={(e) => !disabled && (e.target.style.backgroundColor = v.bg)}",
      "    >",
      "      {text}",
      "    </button>",
      "  );",
      "};",
      "",
      "export default Button;",
      "",
      "// Usage examples:",
      "// <Button text='Click me' onClick={() => {}} />",
      "// <Button text='Delete' variant='danger' onClick={() => {}} />",
      "// <Button text='Submit' variant='success' size='large' />",
      "// <Button text='Cancel' variant='outline' />"
    ],
    "description": "Creates a reusable button component with variants and sizes"
  },
  "Search": {
    "prefix": "erg-search",
    "body": [
      "import React, { useState } from 'react';",
      "",
      "const Search = ({ placeholder = 'Search...', onSearch, onClear }) => {",
      "  const [query, setQuery] = useState('');",
      "",
      "  const handleSearch = () => {",
      "    if (query.trim()) {",
      "      onSearch(query);",
      "    }",
      "  };",
      "",
      "  const handleKeyPress = (e) => {",
      "    if (e.key === 'Enter') {",
      "      handleSearch();",
      "    }",
      "  };",
      "",
      "  const handleClear = () => {",
      "    setQuery('');",
      "    onClear && onClear();",
      "  };",
      "",
      "  return (",
      "    <div style={{ display: 'flex', position: 'relative', maxWidth: '400px' }}>",
      "      <input",
      "        type='text'",
      "        style={{",
      "          flex: 1,",
      "          border: '1px solid #d1d5db',",
      "          borderRadius: '0.375rem 0 0 0.375rem',",
      "          padding: '0.5rem 2.5rem 0.5rem 1rem',",
      "          outline: 'none',",
      "          boxSizing: 'border-box',",
      "        }}",
      "        placeholder={placeholder}",
      "        value={query}",
      "        onChange={(e) => setQuery(e.target.value)}",
      "        onKeyPress={handleKeyPress}",
      "      />",
      "      {query && (",
      "        <button",
      "          onClick={handleClear}",
      "          style={{",
      "            position: 'absolute',",
      "            right: '80px',",
      "            top: '50%',",
      "            transform: 'translateY(-50%)',",
      "            background: 'none',",
      "            border: 'none',",
      "            cursor: 'pointer',",
      "            color: '#9ca3af',",
      "            fontSize: '1rem',",
      "          }}",
      "        >",
      "          ‚úï",
      "        </button>",
      "      )}",
      "      <button",
      "        style={{",
      "          backgroundColor: '#4f46e5',",
      "          color: 'white',",
      "          padding: '0.5rem 1rem',",
      "          borderRadius: '0 0.375rem 0.375rem 0',",
      "          border: 'none',",
      "          cursor: 'pointer',",
      "          transition: 'background-color 0.2s',",
      "        }}",
      "        onClick={handleSearch}",
      "        onMouseOver={(e) => (e.target.style.backgroundColor = '#4338ca')}",
      "        onMouseOut={(e) => (e.target.style.backgroundColor = '#4f46e5')}",
      "      >",
      "        Search",
      "      </button>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Search;"
    ],
    "description": "Creates a reusable search component with clear button"
  },
  "Search Select": {
    "prefix": "erg-search-select",
    "body": [
      "import React, { useState, useRef, useEffect } from 'react';",
      "",
      "const SearchSelect = ({ options, onSelect, placeholder = 'Search...', label }) => {",
      "  const [query, setQuery] = useState('');",
      "  const [isOpen, setIsOpen] = useState(false);",
      "  const [highlightedIndex, setHighlightedIndex] = useState(0);",
      "  const containerRef = useRef(null);",
      "",
      "  const filteredOptions = options.filter(option =>",
      "    option.toString().toLowerCase().includes(query.toLowerCase())",
      "  );",
      "",
      "  useEffect(() => {",
      "    const handleClickOutside = (e) => {",
      "      if (containerRef.current && !containerRef.current.contains(e.target)) {",
      "        setIsOpen(false);",
      "      }",
      "    };",
      "    document.addEventListener('mousedown', handleClickOutside);",
      "    return () => document.removeEventListener('mousedown', handleClickOutside);",
      "  }, []);",
      "",
      "  const handleSelect = (option) => {",
      "    setQuery(option);",
      "    setIsOpen(false);",
      "    onSelect(option);",
      "  };",
      "",
      "  const handleKeyDown = (e) => {",
      "    if (e.key === 'ArrowDown') {",
      "      e.preventDefault();",
      "      setHighlightedIndex(i => Math.min(i + 1, filteredOptions.length - 1));",
      "    } else if (e.key === 'ArrowUp') {",
      "      e.preventDefault();",
      "      setHighlightedIndex(i => Math.max(i - 1, 0));",
      "    } else if (e.key === 'Enter' && filteredOptions[highlightedIndex]) {",
      "      handleSelect(filteredOptions[highlightedIndex]);",
      "    } else if (e.key === 'Escape') {",
      "      setIsOpen(false);",
      "    }",
      "  };",
      "",
      "  return (",
      "    <div style={{ position: 'relative' }} ref={containerRef}>",
      "      {label && <label style={{ display: 'block', color: '#374151', marginBottom: '0.5rem', fontWeight: 500 }}>{label}</label>}",
      "      <input",
      "        type='text'",
      "        style={{ border: '1px solid #d1d5db', borderRadius: '0.375rem', padding: '0.5rem 1rem', width: '100%', outline: 'none', boxSizing: 'border-box' }}",
      "        placeholder={placeholder}",
      "        value={query}",
      "        onFocus={() => setIsOpen(true)}",
      "        onChange={(e) => { setQuery(e.target.value); setHighlightedIndex(0); }}",
      "        onKeyDown={handleKeyDown}",
      "      />",
      "      {isOpen && filteredOptions.length > 0 && (",
      "        <ul style={{ position: 'absolute', backgroundColor: 'white', border: '1px solid #d1d5db', borderRadius: '0.375rem', width: '100%', marginTop: '0.25rem', maxHeight: '12rem', overflow: 'auto', zIndex: 10, listStyle: 'none', padding: 0, margin: 0, boxSizing: 'border-box', boxShadow: '0 4px 6px -1px rgba(0, 0, 0, 0.1)' }}>",
      "          {filteredOptions.map((option, index) => (",
      "            <li",
      "              key={option}",
      "              style={{ padding: '0.5rem 1rem', cursor: 'pointer', backgroundColor: index === highlightedIndex ? '#e0e7ff' : 'white' }}",
      "              onClick={() => handleSelect(option)}",
      "              onMouseEnter={() => setHighlightedIndex(index)}",
      "            >",
      "              {option}",
      "            </li>",
      "          ))}",
      "        </ul>",
      "      )}",
      "      {isOpen && filteredOptions.length === 0 && query && (",
      "        <div style={{ position: 'absolute', backgroundColor: 'white', border: '1px solid #d1d5db', borderRadius: '0.375rem', width: '100%', marginTop: '0.25rem', padding: '0.5rem 1rem', color: '#6b7280', boxSizing: 'border-box' }}>",
      "          No results found",
      "        </div>",
      "      )}",
      "    </div>",
      "  );",
      "};",
      "",
      "export default SearchSelect;"
    ],
    "description": "Creates a reusable search select with keyboard navigation"
  },
  "Modal": {
    "prefix": "erg-modal",
    "body": [
      "import React, { useState, useEffect } from 'react';",
      "",
      "const Modal = ({ title, children, onOk, onCancel, okText = 'Ok', cancelText = 'Cancel', showFooter = true }) => {",
      "  const [isOpen, setIsOpen] = useState(false);",
      "",
      "  useEffect(() => {",
      "    if (isOpen) {",
      "      document.body.style.overflow = 'hidden';",
      "    } else {",
      "      document.body.style.overflow = 'unset';",
      "    }",
      "    return () => { document.body.style.overflow = 'unset'; };",
      "  }, [isOpen]);",
      "",
      "  const openModal = () => setIsOpen(true);",
      "  const closeModal = () => { setIsOpen(false); onCancel && onCancel(); };",
      "  const handleOk = () => { onOk && onOk(); setIsOpen(false); };",
      "",
      "  return (",
      "    <>",
      "      <button",
      "        style={{ padding: '0.5rem 1rem', backgroundColor: '#4f46e5', color: 'white', borderRadius: '0.375rem', border: 'none', cursor: 'pointer', transition: 'background-color 0.2s' }}",
      "        onClick={openModal}",
      "        onMouseOver={(e) => e.target.style.backgroundColor = '#4338ca'}",
      "        onMouseOut={(e) => e.target.style.backgroundColor = '#4f46e5'}",
      "      >",
      "        Open Modal",
      "      </button>",
      "",
      "      {isOpen && (",
      "        <div",
      "          style={{ position: 'fixed', top: 0, right: 0, bottom: 0, left: 0, backgroundColor: 'rgba(0, 0, 0, 0.5)', display: 'flex', alignItems: 'center', justifyContent: 'center', zIndex: 50 }}",
      "          onClick={(e) => e.target === e.currentTarget && closeModal()}",
      "        >",
      "          <div style={{ backgroundColor: 'white', borderRadius: '0.5rem', boxShadow: '0 25px 50px -12px rgba(0, 0, 0, 0.25)', maxWidth: '28rem', width: '90%', maxHeight: '90vh', display: 'flex', flexDirection: 'column' }}>",
      "            {/* Header */}",
      "            <div style={{ display: 'flex', justifyContent: 'space-between', alignItems: 'center', padding: '1rem 1.5rem', borderBottom: '1px solid #e5e7eb' }}>",
      "              <h2 style={{ fontSize: '1.25rem', fontWeight: 600, color: '#1f2937', margin: 0 }}>{title}</h2>",
      "              <button",
      "                style={{ color: '#6b7280', background: 'none', border: 'none', cursor: 'pointer', fontSize: '1.5rem', lineHeight: 1 }}",
      "                onClick={closeModal}",
      "                onMouseOver={(e) => e.target.style.color = '#374151'}",
      "                onMouseOut={(e) => e.target.style.color = '#6b7280'}",
      "              >",
      "                √ó",
      "              </button>",
      "            </div>",
      "",
      "            {/* Body */}",
      "            <div style={{ padding: '1.5rem', overflow: 'auto', flex: 1 }}>",
      "              {children}",
      "            </div>",
      "",
      "            {/* Footer */}",
      "            {showFooter && (",
      "              <div style={{ display: 'flex', justifyContent: 'flex-end', gap: '0.5rem', padding: '1rem 1.5rem', borderTop: '1px solid #e5e7eb' }}>",
      "                <button",
      "                  style={{ backgroundColor: 'white', color: '#374151', padding: '0.5rem 1rem', borderRadius: '0.375rem', border: '1px solid #d1d5db', cursor: 'pointer' }}",
      "                  onClick={closeModal}",
      "                  onMouseOver={(e) => e.target.style.backgroundColor = '#f9fafb'}",
      "                  onMouseOut={(e) => e.target.style.backgroundColor = 'white'}",
      "                >",
      "                  {cancelText}",
      "                </button>",
      "                <button",
      "                  style={{ backgroundColor: '#4f46e5', color: 'white', padding: '0.5rem 1rem', borderRadius: '0.375rem', border: 'none', cursor: 'pointer' }}",
      "                  onClick={handleOk}",
      "                  onMouseOver={(e) => e.target.style.backgroundColor = '#4338ca'}",
      "                  onMouseOut={(e) => e.target.style.backgroundColor = '#4f46e5'}",
      "                >",
      "                  {okText}",
      "                </button>",
      "              </div>",
      "            )}",
      "          </div>",
      "        </div>",
      "      )}",
      "    </>",
      "  );",
      "};",
      "",
      "export default Modal;"
    ],
    "description": "Creates a reusable modal component with customizable footer"
  },
