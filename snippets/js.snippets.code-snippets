  "Pagination": {
    "prefix": "erg-pagination",
    "body": [
      "import React from 'react';",
      "",
      "const Pagination = ({ currentPage, totalPages, onPageChange }) => {",
      "  const getPageNumbers = () => {",
      "    const pages = [];",
      "    const showEllipsis = totalPages > 7;",
      "    ",
      "    if (!showEllipsis) {",
      "      for (let i = 1; i <= totalPages; i++) pages.push(i);",
      "    } else {",
      "      if (currentPage <= 3) {",
      "        for (let i = 1; i <= 4; i++) pages.push(i);",
      "        pages.push('...');",
      "        pages.push(totalPages);",
      "      } else if (currentPage >= totalPages - 2) {",
      "        pages.push(1);",
      "        pages.push('...');",
      "        for (let i = totalPages - 3; i <= totalPages; i++) pages.push(i);",
      "      } else {",
      "        pages.push(1);",
      "        pages.push('...');",
      "        for (let i = currentPage - 1; i <= currentPage + 1; i++) pages.push(i);",
      "        pages.push('...');",
      "        pages.push(totalPages);",
      "      }",
      "    }",
      "    return pages;",
      "  };",
      "",
      "  const buttonStyle = (isActive, isDisabled) => ({",
      "    padding: '0.5rem 0.75rem',",
      "    margin: '0 0.125rem',",
      "    border: '1px solid #d1d5db',",
      "    backgroundColor: isActive ? '#4f46e5' : 'white',",
      "    color: isActive ? 'white' : isDisabled ? '#9ca3af' : '#374151',",
      "    borderRadius: '0.375rem',",
      "    cursor: isDisabled ? 'not-allowed' : 'pointer',",
      "    transition: 'all 0.2s',",
      "  });",
      "",
      "  return (",
      "    <div style={{ display: 'flex', alignItems: 'center', justifyContent: 'center', gap: '0.25rem' }}>",
      "      <button",
      "        style={buttonStyle(false, currentPage === 1)}",
      "        onClick={() => currentPage > 1 && onPageChange(currentPage - 1)}",
      "        disabled={currentPage === 1}",
      "      >",
      "        ‚Üê",
      "      </button>",
      "      ",
      "      {getPageNumbers().map((page, index) => (",
      "        <button",
      "          key={index}",
      "          style={buttonStyle(page === currentPage, page === '...')}",
      "          onClick={() => typeof page === 'number' && onPageChange(page)}",
      "          disabled={page === '...'}",
      "        >",
      "          {page}",
      "        </button>",
      "      ))}",
      "      ",
      "      <button",
      "        style={buttonStyle(false, currentPage === totalPages)}",
      "        onClick={() => currentPage < totalPages && onPageChange(currentPage + 1)}",
      "        disabled={currentPage === totalPages}",
      "      >",
      "        ‚Üí",
      "      </button>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Pagination;",
      "",
      "// Usage:",
      "// <Pagination currentPage={1} totalPages={10} onPageChange={(page) => console.log(page)} />"
    ],
    "description": "Creates a pagination component with ellipsis"
  },
  "Avatar": {
    "prefix": "erg-avatar",
    "body": [
      "import React from 'react';",
      "",
      "const Avatar = ({ src, name, size = 'medium', status }) => {",
      "  const sizes = {",
      "    small: { width: '32px', height: '32px', fontSize: '0.75rem' },",
      "    medium: { width: '40px', height: '40px', fontSize: '0.875rem' },",
      "    large: { width: '56px', height: '56px', fontSize: '1.25rem' },",
      "    xlarge: { width: '80px', height: '80px', fontSize: '1.5rem' },",
      "  };",
      "",
      "  const statusColors = {",
      "    online: '#22c55e',",
      "    offline: '#9ca3af',",
      "    busy: '#ef4444',",
      "    away: '#f59e0b',",
      "  };",
      "",
      "  const s = sizes[size] || sizes.medium;",
      "  const initials = name ? name.split(' ').map(n => n[0]).join('').toUpperCase().slice(0, 2) : '?';",
      "",
      "  return (",
      "    <div style={{ position: 'relative', display: 'inline-block' }}>",
      "      {src ? (",
      "        <img",
      "          src={src}",
      "          alt={name}",
      "          style={{",
      "            ...s,",
      "            borderRadius: '50%',",
      "            objectFit: 'cover',",
      "          }}",
      "        />",
      "      ) : (",
      "        <div",
      "          style={{",
      "            ...s,",
      "            borderRadius: '50%',",
      "            backgroundColor: '#4f46e5',",
      "            color: 'white',",
      "            display: 'flex',",
      "            alignItems: 'center',",
      "            justifyContent: 'center',",
      "            fontWeight: 600,",
      "          }}",
      "        >",
      "          {initials}",
      "        </div>",
      "      )}",
      "      {status && (",
      "        <span",
      "          style={{",
      "            position: 'absolute',",
      "            bottom: 0,",
      "            right: 0,",
      "            width: '25%',",
      "            height: '25%',",
      "            backgroundColor: statusColors[status] || statusColors.offline,",
      "            borderRadius: '50%',",
      "            border: '2px solid white',",
      "          }}",
      "        />",
      "      )}",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Avatar;",
      "",
      "// Usage:",
      "// <Avatar src='https://via.placeholder.com/150' name='John Doe' status='online' />",
      "// <Avatar name='Jane Smith' size='large' status='busy' />"
    ],
    "description": "Creates an avatar component with status indicator"
  },
  "Badge": {
    "prefix": "erg-badge",
    "body": [
      "import React from 'react';",
      "",
      "const Badge = ({ children, variant = 'default', size = 'medium' }) => {",
      "  const variants = {",
      "    default: { bg: '#e5e7eb', color: '#374151' },",
      "    primary: { bg: '#e0e7ff', color: '#4338ca' },",
      "    success: { bg: '#dcfce7', color: '#166534' },",
      "    warning: { bg: '#fef3c7', color: '#92400e' },",
      "    danger: { bg: '#fee2e2', color: '#991b1b' },",
      "    info: { bg: '#dbeafe', color: '#1e40af' },",
      "  };",
      "",
      "  const sizes = {",
      "    small: { padding: '0.125rem 0.5rem', fontSize: '0.75rem' },",
      "    medium: { padding: '0.25rem 0.75rem', fontSize: '0.875rem' },",
      "    large: { padding: '0.375rem 1rem', fontSize: '1rem' },",
      "  };",
      "",
      "  const v = variants[variant] || variants.default;",
      "  const s = sizes[size] || sizes.medium;",
      "",
      "  return (",
      "    <span",
      "      style={{",
      "        display: 'inline-flex',",
      "        alignItems: 'center',",
      "        borderRadius: '9999px',",
      "        fontWeight: 500,",
      "        backgroundColor: v.bg,",
      "        color: v.color,",
      "        ...s,",
      "      }}",
      "    >",
      "      {children}",
      "    </span>",
      "  );",
      "};",
      "",
      "export default Badge;",
      "",
      "// Usage:",
      "// <Badge>Default</Badge>",
      "// <Badge variant='success'>Active</Badge>",
      "// <Badge variant='danger' size='small'>Error</Badge>"
    ],
    "description": "Creates a badge/tag component with variants"
  },
  "Accordion": {
    "prefix": "erg-accordion",
    "body": [
      "import React, { useState } from 'react';",
      "",
      "const AccordionItem = ({ title, children, isOpen, onToggle }) => (",
      "  <div style={{ borderBottom: '1px solid #e5e7eb' }}>",
      "    <button",
      "      onClick={onToggle}",
      "      style={{",
      "        width: '100%',",
      "        padding: '1rem',",
      "        display: 'flex',",
      "        justifyContent: 'space-between',",
      "        alignItems: 'center',",
      "        backgroundColor: 'white',",
      "        border: 'none',",
      "        cursor: 'pointer',",
      "        textAlign: 'left',",
      "      }}",
      "    >",
      "      <span style={{ fontWeight: 500, color: '#1f2937' }}>{title}</span>",
      "      <span style={{ transform: isOpen ? 'rotate(180deg)' : 'rotate(0)', transition: 'transform 0.2s' }}>",
      "        ‚ñº",
      "      </span>",
      "    </button>",
      "    <div",
      "      style={{",
      "        maxHeight: isOpen ? '500px' : '0',",
      "        overflow: 'hidden',",
      "        transition: 'max-height 0.3s ease',",
      "      }}",
      "    >",
      "      <div style={{ padding: '0 1rem 1rem', color: '#4b5563' }}>",
      "        {children}",
      "      </div>",
      "    </div>",
      "  </div>",
      ");",
      "",
      "const Accordion = ({ items, allowMultiple = false }) => {",
      "  const [openItems, setOpenItems] = useState([]);",
      "",
      "  const toggleItem = (index) => {",
      "    if (allowMultiple) {",
      "      setOpenItems(prev => ",
      "        prev.includes(index) ? prev.filter(i => i !== index) : [...prev, index]",
      "      );",
      "    } else {",
      "      setOpenItems(prev => prev.includes(index) ? [] : [index]);",
      "    }",
      "  };",
      "",
      "  return (",
      "    <div style={{ border: '1px solid #e5e7eb', borderRadius: '0.5rem', overflow: 'hidden' }}>",
      "      {items.map((item, index) => (",
      "        <AccordionItem",
      "          key={index}",
      "          title={item.title}",
      "          isOpen={openItems.includes(index)}",
      "          onToggle={() => toggleItem(index)}",
      "        >",
      "          {item.content}",
      "        </AccordionItem>",
      "      ))}",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Accordion;",
      "",
      "// Usage:",
      "// const items = [",
      "//   { title: 'Section 1', content: 'Content for section 1' },",
      "//   { title: 'Section 2', content: 'Content for section 2' },",
      "//   { title: 'Section 3', content: 'Content for section 3' },",
      "// ];",
      "// <Accordion items={items} />",
      "// <Accordion items={items} allowMultiple />"
    ],
    "description": "Creates an accordion/collapsible component"
  },
  "Toast": {
    "prefix": "erg-toast",
    "body": [
      "import React, { useState, useEffect, createContext, useContext } from 'react';",
      "",
      "const ToastContext = createContext();",
      "",
      "const Toast = ({ message, type, onClose }) => {",
      "  const types = {",
      "    success: { bg: '#f0fdf4', border: '#22c55e', color: '#166534', icon: '‚úì' },",
      "    error: { bg: '#fef2f2', border: '#ef4444', color: '#991b1b', icon: '‚úï' },",
      "    warning: { bg: '#fffbeb', border: '#f59e0b', color: '#92400e', icon: '‚ö†' },",
      "    info: { bg: '#eff6ff', border: '#3b82f6', color: '#1e40af', icon: '‚Ñπ' },",
      "  };",
      "  const t = types[type] || types.info;",
      "",
      "  useEffect(() => {",
      "    const timer = setTimeout(onClose, 5000);",
      "    return () => clearTimeout(timer);",
      "  }, [onClose]);",
      "",
      "  return (",
      "    <div style={{",
      "      display: 'flex', alignItems: 'center', gap: '0.75rem',",
      "      padding: '1rem', backgroundColor: t.bg, borderLeft: `4px solid ${t.border}`,",
      "      borderRadius: '0.375rem', boxShadow: '0 4px 6px rgba(0,0,0,0.1)',",
      "      marginBottom: '0.5rem', animation: 'slideIn 0.3s ease',",
      "    }}>",
      "      <span style={{ fontSize: '1.25rem' }}>{t.icon}</span>",
      "      <span style={{ flex: 1, color: t.color }}>{message}</span>",
      "      <button onClick={onClose} style={{ background: 'none', border: 'none', color: t.color, cursor: 'pointer', fontSize: '1.25rem' }}>√ó</button>",
      "      <style>{`@keyframes slideIn { from { transform: translateX(100%); opacity: 0; } to { transform: translateX(0); opacity: 1; } }`}</style>",
      "    </div>",
      "  );",
      "};",
      "",
      "export const ToastProvider = ({ children }) => {",
      "  const [toasts, setToasts] = useState([]);",
      "",
      "  const addToast = (message, type = 'info') => {",
      "    const id = Date.now();",
      "    setToasts(prev => [...prev, { id, message, type }]);",
      "  };",
      "",
      "  const removeToast = (id) => setToasts(prev => prev.filter(t => t.id !== id));",
      "",
      "  return (",
      "    <ToastContext.Provider value={{ addToast }}>",
      "      {children}",
      "      <div style={{ position: 'fixed', top: '1rem', right: '1rem', zIndex: 100, maxWidth: '400px' }}>",
      "        {toasts.map(toast => (",
      "          <Toast key={toast.id} {...toast} onClose={() => removeToast(toast.id)} />",
      "        ))}",
      "      </div>",
      "    </ToastContext.Provider>",
      "  );",
      "};",
      "",
      "export const useToast = () => useContext(ToastContext);",
      "",
      "// Usage:",
      "// 1. Wrap your app: <ToastProvider><App /></ToastProvider>",
      "// 2. In any component: const { addToast } = useToast();",
      "// 3. Show toast: addToast('Success!', 'success');"
    ],
    "description": "Creates a toast notification system with context"
  }
}
