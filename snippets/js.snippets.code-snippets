  "Alert": {
    "prefix": "erg-alert",
    "body": [
      "import React, { useState } from 'react';",
      "",
      "const Alert = ({ type = 'info', title, message, dismissible = true, onDismiss }) => {",
      "  const [isVisible, setIsVisible] = useState(true);",
      "",
      "  const types = {",
      "    info: { bg: '#eff6ff', border: '#3b82f6', text: '#1e40af', icon: '‚Ñπ' },",
      "    success: { bg: '#f0fdf4', border: '#22c55e', text: '#166534', icon: '‚úì' },",
      "    warning: { bg: '#fffbeb', border: '#f59e0b', text: '#92400e', icon: '‚ö†' },",
      "    error: { bg: '#fef2f2', border: '#ef4444', text: '#991b1b', icon: '‚úï' },",
      "  };",
      "",
      "  const t = types[type] || types.info;",
      "",
      "  const handleDismiss = () => {",
      "    setIsVisible(false);",
      "    onDismiss && onDismiss();",
      "  };",
      "",
      "  if (!isVisible) return null;",
      "",
      "  return (",
      "    <div",
      "      style={{",
      "        backgroundColor: t.bg,",
      "        borderLeft: `4px solid ${t.border}`,",
      "        borderRadius: '0.375rem',",
      "        padding: '1rem',",
      "        display: 'flex',",
      "        alignItems: 'flex-start',",
      "        gap: '0.75rem',",
      "      }}",
      "    >",
      "      <span style={{ fontSize: '1.25rem' }}>{t.icon}</span>",
      "      <div style={{ flex: 1 }}>",
      "        {title && <h4 style={{ fontWeight: 600, color: t.text, marginBottom: '0.25rem' }}>{title}</h4>}",
      "        <p style={{ color: t.text, margin: 0 }}>{message}</p>",
      "      </div>",
      "      {dismissible && (",
      "        <button",
      "          onClick={handleDismiss}",
      "          style={{ background: 'none', border: 'none', color: t.text, cursor: 'pointer', fontSize: '1.25rem', padding: 0 }}",
      "        >",
      "          √ó",
      "        </button>",
      "      )}",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Alert;",
      "",
      "// Usage:",
      "// <Alert type='success' title='Success!' message='Your changes have been saved.' />",
      "// <Alert type='error' message='Something went wrong.' />",
      "// <Alert type='warning' title='Warning' message='Please review your input.' />"
    ],
    "description": "Creates a dismissible alert/notification component"
