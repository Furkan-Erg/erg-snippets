      "const Spinner = ({ size = 'medium', color = '#4f46e5', text }) => {",
      "  const sizes = {",
      "    small: { width: '16px', height: '16px', border: '2px' },",
      "    medium: { width: '32px', height: '32px', border: '3px' },",
      "    large: { width: '48px', height: '48px', border: '4px' },",
      "  };",
      "",
      "  const s = sizes[size] || sizes.medium;",
      "",
      "  return (",
      "    <div style={{ display: 'flex', flexDirection: 'column', alignItems: 'center', gap: '0.5rem' }}>",
      "      <div",
      "        style={{",
      "          width: s.width,",
      "          height: s.height,",
      "          border: `${s.border} solid #e5e7eb`,",
      "          borderTopColor: color,",
      "          borderRadius: '50%',",
      "          animation: 'spin 1s linear infinite',",
      "        }}",
      "      />",
      "      {text && <span style={{ color: '#6b7280', fontSize: '0.875rem' }}>{text}</span>}",
      "      <style>{`",
      "        @keyframes spin {",
      "          to { transform: rotate(360deg); }",
      "        }",
      "      `}</style>",
      "    </div>",
      "  );",
      "};",
