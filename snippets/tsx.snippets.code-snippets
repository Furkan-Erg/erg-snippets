      "      ) : (",
      "        <div style={{ ...s, borderRadius: '50%', backgroundColor: '#4f46e5', color: 'white', display: 'flex', alignItems: 'center', justifyContent: 'center', fontWeight: 600 }}>{initials}</div>",
      "      )}",
      "      {status && <span style={{ position: 'absolute', bottom: 0, right: 0, width: '25%', height: '25%', backgroundColor: statusColors[status], borderRadius: '50%', border: '2px solid white' }} />}",
      "    </div>",
      "  );",
