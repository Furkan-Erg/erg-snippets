  "Search": {
    "prefix": "erg-search",
    "body": [
      "import React, { useState, KeyboardEvent } from 'react';",
      "",
      "interface SearchProps {",
      "  placeholder?: string;",
      "  onSearch: (query: string) => void;",
      "  onClear?: () => void;",
      "}",
