      "import React from 'react';",
      "",
      "const NotFoundPage: React.FC = () => {",
      "  const goHome = () => window.location.replace('/');",
      "  const goBack = () => window.history.back();",
      "",
      "  return (",
      "    <div style={{ display: 'flex', alignItems: 'center', justifyContent: 'center', height: '100vh', backgroundColor: '#f3f4f6' }}>",
      "      <div style={{ textAlign: 'center', padding: '2rem' }}>",
      "        <h1 style={{ fontSize: '6rem', fontWeight: 700, color: '#4f46e5', marginBottom: '0.5rem', lineHeight: 1 }}>404</h1>",
      "        <h2 style={{ fontSize: '1.5rem', fontWeight: 600, color: '#1f2937', marginBottom: '0.5rem' }}>Page Not Found</h2>",
      "        <p style={{ fontSize: '1rem', color: '#6b7280', marginBottom: '2rem', maxWidth: '400px' }}>",
      "          Sorry, we couldn't find the page you're looking for.",
      "        </p>",
      "        <div style={{ display: 'flex', gap: '1rem', justifyContent: 'center' }}>",
      "          <button onClick={goBack} style={{ padding: '0.5rem 1.5rem', backgroundColor: 'white', color: '#4f46e5', borderRadius: '0.375rem', border: '1px solid #4f46e5', cursor: 'pointer' }}>Go Back</button>",
      "          <button onClick={goHome} style={{ padding: '0.5rem 1.5rem', backgroundColor: '#4f46e5', color: 'white', borderRadius: '0.375rem', border: 'none', cursor: 'pointer' }}>Go to Homepage</button>",
      "        </div>",
      "      </div>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default NotFoundPage;"
    ],
    "description": "Creates a 404 Not Found page"
