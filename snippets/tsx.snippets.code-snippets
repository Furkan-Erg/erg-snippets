  "Pagination": {
    "prefix": "erg-pagination",
    "body": [
      "import React from 'react';",
      "",
      "interface PaginationProps {",
      "  currentPage: number;",
      "  totalPages: number;",
      "  onPageChange: (page: number) => void;",
      "}",
      "",
      "const Pagination: React.FC<PaginationProps> = ({ currentPage, totalPages, onPageChange }) => {",
      "  const getPageNumbers = (): (number | string)[] => {",
      "    const pages: (number | string)[] = [];",
      "    if (totalPages <= 7) { for (let i = 1; i <= totalPages; i++) pages.push(i); }",
      "    else if (currentPage <= 3) { for (let i = 1; i <= 4; i++) pages.push(i); pages.push('...'); pages.push(totalPages); }",
      "    else if (currentPage >= totalPages - 2) { pages.push(1); pages.push('...'); for (let i = totalPages - 3; i <= totalPages; i++) pages.push(i); }",
      "    else { pages.push(1, '...'); for (let i = currentPage - 1; i <= currentPage + 1; i++) pages.push(i); pages.push('...', totalPages); }",
      "    return pages;",
      "  };",
      "",
      "  const btnStyle = (active: boolean, disabled: boolean): React.CSSProperties => ({",
      "    padding: '0.5rem 0.75rem', margin: '0 0.125rem', border: '1px solid #d1d5db',",
      "    backgroundColor: active ? '#4f46e5' : 'white', color: active ? 'white' : disabled ? '#9ca3af' : '#374151',",
      "    borderRadius: '0.375rem', cursor: disabled ? 'not-allowed' : 'pointer',",
      "  });",
      "",
      "  return (",
      "    <div style={{ display: 'flex', alignItems: 'center', justifyContent: 'center', gap: '0.25rem' }}>",
      "      <button style={btnStyle(false, currentPage === 1)} onClick={() => currentPage > 1 && onPageChange(currentPage - 1)} disabled={currentPage === 1}>←</button>",
      "      {getPageNumbers().map((page, i) => (",
      "        <button key={i} style={btnStyle(page === currentPage, page === '...')} onClick={() => typeof page === 'number' && onPageChange(page)} disabled={page === '...'}>{page}</button>",
      "      ))}",
      "      <button style={btnStyle(false, currentPage === totalPages)} onClick={() => currentPage < totalPages && onPageChange(currentPage + 1)} disabled={currentPage === totalPages}>→</button>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default Pagination;"
    ],
    "description": "Creates a pagination component with TypeScript"
  },
  "Avatar": {
    "prefix": "erg-avatar",
