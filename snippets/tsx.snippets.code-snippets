      "import React from 'react';",
      "",
      "interface AvatarProps {",
      "  src?: string;",
      "  name?: string;",
      "  size?: 'small' | 'medium' | 'large' | 'xlarge';",
      "  status?: 'online' | 'offline' | 'busy' | 'away';",
      "}",
      "",
      "const Avatar: React.FC<AvatarProps> = ({ src, name, size = 'medium', status }) => {",
      "  const sizes = { small: { width: '32px', height: '32px', fontSize: '0.75rem' }, medium: { width: '40px', height: '40px', fontSize: '0.875rem' }, large: { width: '56px', height: '56px', fontSize: '1.25rem' }, xlarge: { width: '80px', height: '80px', fontSize: '1.5rem' } };",
      "  const statusColors: Record<string, string> = { online: '#22c55e', offline: '#9ca3af', busy: '#ef4444', away: '#f59e0b' };",
      "  const s = sizes[size];",
