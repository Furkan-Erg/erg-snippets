  "Login Page": {
    "prefix": "erg-login",
    "body": [
      "import { Component } from '@angular/core';",
      "import { FormBuilder, FormGroup, ReactiveFormsModule, Validators } from '@angular/forms';",
      "",
      "@Component({",
      "  selector: 'app-login',",
      "  template: `",
      "    <div class='container'>",
      "      <div class='card'>",
      "        <h2 class='title'>Login</h2>",
      "        <form [formGroup]='loginForm' (ngSubmit)='handleLogin()'>",
      "          <div class='form-group'>",
      "            <label for='username' class='label'>Username</label>",
      "            <input",
      "              id='username'",
      "              type='text'",
      "              class='input'",
      "              formControlName='username'",
      "              required",
      "            />",
      "          </div>",
      "          <div class='form-group-last'>",
      "            <label for='password' class='label'>Password</label>",
      "            <input",
      "              id='password'",
      "              type='password'",
      "              class='input'",
      "              formControlName='password'",
      "              required",
      "            />",
      "          </div>",
      "          <button type='submit' class='btn-primary'>",
      "            Login",
      "          </button>",
      "        </form>",
      "      </div>",
      "    </div>",
      "  `,",
      "  styles: [`",
      "    .container { display: flex; align-items: center; justify-content: center; min-height: 100vh; background-color: #f3f4f6; }",
      "    .card { background-color: white; padding: 2rem; border-radius: 0.5rem; box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1); max-width: 24rem; width: 100%; }",
      "    .title { font-size: 1.5rem; font-weight: 600; margin-bottom: 1.5rem; text-align: center; color: #374151; }",
      "    .form-group { margin-bottom: 1rem; }",
      "    .form-group-last { margin-bottom: 1.5rem; }",
      "    .label { display: block; color: #4b5563; margin-bottom: 0.5rem; }",
      "    .input { width: 100%; padding: 0.5rem 1rem; border: 1px solid #d1d5db; border-radius: 0.375rem; outline: none; box-sizing: border-box; }",
      "    .input:focus { box-shadow: 0 0 0 2px #6366f1; }",
      "    .btn-primary { width: 100%; background-color: #4f46e5; color: white; padding: 0.5rem; border-radius: 0.375rem; border: none; cursor: pointer; transition: background-color 0.2s; }",
      "    .btn-primary:hover { background-color: #4338ca; }",
      "  `],",
      "  standalone: true,",
      "  imports: [ReactiveFormsModule],",
      "})",
      "export class LoginComponent {",
      "  loginForm: FormGroup;",
      "",
      "  constructor(private fb: FormBuilder) {",
      "    this.loginForm = this.fb.group({",
      "      username: ['', Validators.required],",
      "      password: ['', Validators.required]",
      "    });",
      "  }",
      "",
      "  async handleLogin() {",
      "    const { username, password } = this.loginForm.value;",
      "    try {",
      "      const response = await fetch('example-url', {",
      "        method: 'POST',",
      "        headers: {",
      "          'Content-Type': 'application/json',",
      "        },",
      "        body: JSON.stringify({ username, password }),",
      "      });",
      "      const data = await response.json();",
      "      console.log('Response:', data);",
      "    } catch (error) {",
      "      console.error('Error:', error);",
      "    }",
      "  }",
      "}"
    ],
    "description": "This will create a certain login page for you"
