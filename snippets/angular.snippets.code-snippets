  "Table": {
    "prefix": "erg-table",
    "body": [
      "import { CommonModule } from '@angular/common';",
      "import { Component, Input } from '@angular/core';",
      "",
      "@Component({",
      "  selector: 'app-table',",
      "  template: `",
      "    <div class='table-container'>",
      "      <table class='table'>",
      "        <thead>",
      "          <tr>",
      "            <th *ngFor='let col of columns' class='table-header' (click)='handleSort(col.key)'>",
      "              {{ col.label }}",
      "              <span *ngIf='sortKey === col.key'>{{ sortDirection === \"asc\" ? \" ▲\" : \" ▼\" }}</span>",
      "            </th>",
      "          </tr>",
      "        </thead>",
      "        <tbody>",
      "          <tr *ngFor='let row of sortedData' class='table-row'>",
      "            <td *ngFor='let col of columns' class='table-cell'>{{ row[col.key] }}</td>",
      "          </tr>",
      "        </tbody>",
      "      </table>",
      "    </div>",
      "  `,",
      "  styles: [`",
      "    .table-container { overflow-x: auto; }",
      "    .table { width: 100%; border-collapse: collapse; }",
      "    .table-header { padding: 0.75rem 1rem; text-align: left; font-weight: 600; color: #374151; background-color: #f9fafb; border-bottom: 2px solid #e5e7eb; cursor: pointer; user-select: none; }",
      "    .table-header:hover { background-color: #f3f4f6; }",
      "    .table-row { border-bottom: 1px solid #e5e7eb; }",
      "    .table-row:hover { background-color: #f9fafb; }",
      "    .table-cell { padding: 0.75rem 1rem; color: #4b5563; }",
      "  `],",
      "  standalone: true,",
      "  imports: [CommonModule],",
      "})",
      "export class TableComponent {",
      "  @Input() columns: { key: string; label: string }[] = [];",
      "  @Input() data: any[] = [];",
      "  sortKey: string = '';",
      "  sortDirection: 'asc' | 'desc' = 'asc';",
      "",
      "  get sortedData() {",
      "    if (!this.sortKey) return this.data;",
      "    return [...this.data].sort((a, b) => {",
      "      const aVal = a[this.sortKey];",
      "      const bVal = b[this.sortKey];",
      "      if (aVal < bVal) return this.sortDirection === 'asc' ? -1 : 1;",
      "      if (aVal > bVal) return this.sortDirection === 'asc' ? 1 : -1;",
      "      return 0;",
      "    });",
      "  }",
      "",
      "  handleSort(key: string) {",
      "    if (this.sortKey === key) {",
      "      this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';",
      "    } else {",
      "      this.sortKey = key;",
      "      this.sortDirection = 'asc';",
      "    }",
      "  }",
      "}"
    ],
    "description": "This will create a sortable table component"
  },
