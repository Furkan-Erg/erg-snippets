  "Pagination": {
    "prefix": "erg-pagination",
    "body": [
      "import { CommonModule } from '@angular/common';",
      "import { Component, EventEmitter, Input, Output } from '@angular/core';",
      "",
      "@Component({",
      "  selector: 'app-pagination',",
      "  template: `",
      "    <nav class='pagination'>",
      "      <button class='page-btn' [disabled]='currentPage === 1' (click)='handlePageChange(currentPage - 1)'>Previous</button>",
      "      <ng-container *ngFor='let page of pages'>",
      "        <span *ngIf='page === \"...\"' class='ellipsis'>...</span>",
      "        <button *ngIf='page !== \"...\"' class='page-btn' [class.active]='page === currentPage' (click)='handlePageChange(page)'>{{ page }}</button>",
      "      </ng-container>",
      "      <button class='page-btn' [disabled]='currentPage === totalPages' (click)='handlePageChange(currentPage + 1)'>Next</button>",
      "    </nav>",
      "  `,",
      "  styles: [`",
      "    .pagination { display: flex; align-items: center; gap: 0.25rem; }",
      "    .page-btn { padding: 0.5rem 0.75rem; border: 1px solid #d1d5db; background-color: white; color: #374151; border-radius: 0.375rem; cursor: pointer; transition: all 0.2s; }",
      "    .page-btn:hover:not(:disabled) { background-color: #f3f4f6; }",
      "    .page-btn:disabled { opacity: 0.5; cursor: not-allowed; }",
      "    .page-btn.active { background-color: #4f46e5; color: white; border-color: #4f46e5; }",
      "    .ellipsis { padding: 0.5rem; color: #6b7280; }",
      "  `],",
      "  standalone: true,",
      "  imports: [CommonModule],",
      "})",
      "export class PaginationComponent {",
      "  @Input() currentPage: number = 1;",
      "  @Input() totalPages: number = 1;",
      "  @Output() pageChange = new EventEmitter<number>();",
      "",
      "  get pages(): (number | string)[] {",
      "    const pages: (number | string)[] = [];",
      "    if (this.totalPages <= 7) {",
      "      for (let i = 1; i <= this.totalPages; i++) pages.push(i);",
      "    } else {",
      "      pages.push(1);",
      "      if (this.currentPage > 3) pages.push('...');",
      "      for (let i = Math.max(2, this.currentPage - 1); i <= Math.min(this.totalPages - 1, this.currentPage + 1); i++) pages.push(i);",
      "      if (this.currentPage < this.totalPages - 2) pages.push('...');",
      "      pages.push(this.totalPages);",
      "    }",
      "    return pages;",
      "  }",
      "",
      "  handlePageChange(page: number | string) {",
      "    if (typeof page === 'number' && page >= 1 && page <= this.totalPages) {",
      "      this.pageChange.emit(page);",
      "    }",
      "  }",
      "}"
    ],
    "description": "This will create a pagination component"
  },
