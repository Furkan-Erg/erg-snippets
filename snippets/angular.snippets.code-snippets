      "import { FormsModule } from '@angular/forms';",
      "",
      "@Component({",
      "  selector: 'app-search',",
      "  template: `",
      "    <div class='search-container'>",
      "      <input",
      "        type='text'",
      "        class='search-input'",
      "        [placeholder]='placeholder'",
      "        [(ngModel)]='query'",
      "      />",
      "      <button class='search-btn' (click)='onSearch()'>",
      "        Search",
      "      </button>",
      "    </div>",
      "  `,",
      "  styles: [`",
      "    .search-container { display: flex; }",
      "    .search-input { border: 1px solid #d1d5db; border-radius: 0.375rem 0 0 0.375rem; padding: 0.5rem 1rem; width: 100%; outline: none; box-sizing: border-box; }",
      "    .search-input:focus { box-shadow: 0 0 0 2px #6366f1; }",
      "    .search-btn { background-color: #4f46e5; color: white; padding: 0.5rem 1rem; border-radius: 0 0.375rem 0.375rem 0; border: none; cursor: pointer; transition: background-color 0.2s; }",
      "    .search-btn:hover { background-color: #4338ca; }",
      "  `],",
      "  standalone: true,",
      "  imports: [FormsModule],",
      "})",
      "export class SearchComponent {",
      "  @Input() placeholder: string = 'Search...';",
      "  @Output() search = new EventEmitter<string>();",
      "  query: string = '';",
      "",
      "  onSearch() {",
      "    this.search.emit(this.query);",
      "    this.query = '';",
      "  }",
      "}"
    ],
    "description": "This will create a reusable search component"
